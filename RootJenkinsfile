pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        script {
          echo "build jenkins file is running"
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo "root jenkins file is running"
        }
      }
    }
  }
}