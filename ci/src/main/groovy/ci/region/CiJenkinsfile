package ci.region

def module

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        script {
          module = load "script/StaticScript.groovy"
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getStaticString()
        }
      }
    }
  }
}