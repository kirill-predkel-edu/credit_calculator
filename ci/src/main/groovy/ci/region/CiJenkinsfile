package ci.region

import script.*

def module

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          module = new StaticScript()
          echo module.getStaticString()
        }
      }
    }
  }
}