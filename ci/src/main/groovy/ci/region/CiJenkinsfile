package ci.region

import script.EchoScript

def module

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        script {
          module = new EchoScript()
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getBuildString()
        }
      }
    }
  }
}