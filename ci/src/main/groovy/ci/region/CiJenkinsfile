package ci.region

import script.EchoScript
import script.StaticScript

def test = new EchoScript()

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        script {
          echo test.getBuildString()
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo StaticScript.getStaticString()
        }
      }
    }
  }
}