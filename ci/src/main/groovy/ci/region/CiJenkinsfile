package ci.region

import script.*

def module = new EchoScript()

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getBuildString()
        }
      }
    }
  }
}