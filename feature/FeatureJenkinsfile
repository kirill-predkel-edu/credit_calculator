@Library(['Shared1', 'Shared2'])
import main.groovy.script.*

def module
def z

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        script {
          z = new FirstScript()
          echo z.a()
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getStaticString()
        }
      }
    }
  }
}