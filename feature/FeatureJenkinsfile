@Library(['Shared1', 'Shared2']) _

def module
def z

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        z = new _.FirstScript()
        echo z.a()
        echo 'Building..'
        stress()
        helloworld()
        script {
          module = load "ci/src/main/groovy/script/StaticScript.groovy"
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getStaticString()
        }
      }
    }
  }
}