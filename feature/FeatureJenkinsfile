@Library("Shared1") l1
@Library("Shared2") l2
def module

pipeline {
  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        stress()
        def a = helloWorld()
        println(a)
        script {
          module = load "ci/src/main/groovy/script/StaticScript.groovy"
        }
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
        script {
          echo module.getStaticString()
        }
      }
    }
  }
}